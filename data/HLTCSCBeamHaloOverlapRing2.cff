module level1seedHLTCSCBeamHaloOverlapRing2 = hltLevel1GTSeed from "HLTrigger/HLTfilters/data/hltLevel1GTSeed.cfi"
replace level1seedHLTCSCBeamHaloOverlapRing2.L1SeedsLogicalExpression = "L1_SingleMuHalo"

module prescaleHLTCSCBeamHaloOverlapRing2 = hltPrescaler from "HLTrigger/HLTcore/data/hltPrescaler.cfi"
replace prescaleHLTCSCBeamHaloOverlapRing2.prescaleFactor = 1

module overlapsHLTCSCBeamHaloOverlapRing2 = HLTCSCOverlapFilter {
    InputTag input = csc2DRecHits
    uint32 minHits = 4
    double xWindow = 2.
    double yWindow = 2.
    bool ring1 = false
    bool ring2 = true
    bool fillHists = false
}

sequence HLTCSCBeamHaloOverlapRing2 = {level1seedHLTCSCBeamHaloOverlapRing2, prescaleHLTCSCBeamHaloOverlapRing2, overlapsHLTCSCBeamHaloOverlapRing2}

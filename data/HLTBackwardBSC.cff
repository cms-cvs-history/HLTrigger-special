module level1seedHLTBackwardBSC = hltLevel1GTSeed from "HLTrigger/HLTfilters/data/hltLevel1GTSeed.cfi"
replace level1seedHLTBackwardBSC.L1TechTriggerSeeding = true
replace level1seedHLTBackwardBSC.L1SeedsLogicalExpression = "2"

module prescaleHLTBackwardBSC = hltPrescaler from "HLTrigger/HLTcore/data/hltPrescaler.cfi"
replace prescaleHLTBackwardBSC.prescaleFactor = 1

sequence HLTBackwardBSC = {level1seedHLTBackwardBSC, prescaleHLTBackwardBSC}
